<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyStore</title>
    <script src="https://cdn.tailwindcss.com"></script>
   
</head>
<body>
    <header>
        <nav class="mb-3 bg-gray-300">
            <div class="flex justify-center items-center h-12 text-lg ">
                
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    
                </button>
                <div class=" ">
                    <a class="mr-3 font-extrabold no-underline text-xl font-sans" asp-area="" asp-page="/Index">MyStore</a>

                      
                        <a class="mr-3 no-underline" asp-area="" asp-page="/Index">Home</a>


                        <a class="mr-3 no-underline" asp-area="" asp-page="/Privacy">Privacy</a>

                        <a class=" no-underline" asp-area="" asp-page="./Clients/Index">Clients</a>
                    
                  

                   
                       
                        
                    </ul>
                  
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

  

    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>


<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
<link rel="stylesheet" href="~/MyStore.styles.css" asp-append-version="true" />